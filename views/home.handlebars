 <!--<h1>Open Home Schedule</h1>!-->
    <div id="wrapper">
        <div id="sidebar-wrapper">

            <img src="img/trademe_logo.png" alt="TradeMe Open Home Scheduler" class="logo">

            <ul class="sidebar-nav">
                
                <p id="msg"></p>
                {{#each watchlist}}

                    <div class = "PropertyListItem">
                    <li class="sidebar-brand"><span>{{title}}</span> <br>
                    <span class="sidebar-brand" id="location">{{location}}</span><br>
                    <span style="display:none;" id="geolocation">{{geolocation}}</span>

                    {{#each visitTimeList}}<br>
                        <span class="sidebar-brand">{{startTime}}</span><br>
                        <span class="sidebar-brand">{{endTime}}</span>
                    {{/each}}
                    </li>
                    </div>

                {{/each}}
            </ul>

        </div>

        <div class="map">
            <a href="#menu-toggle" class="btn btn-default" id="menu-toggle">&#9776</a>

            <div id="map-canvas"></div>
        </div>
    </div>
